<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">
	<xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />
    <link name="robot_footprint"></link>

    <joint name="robot_footprint_joint" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0" />
      <parent link="robot_footprint"/>
      <child link="chassis" />
    </joint>

    <link name='chassis'>
        <pose>0 0 0.1 0 0 0</pose>
        <inertial>
            <mass value="15.0"/>
            <inertia
                ixx="0.1" ixy="0" ixz="0"
                iyy="0.1" iyz="0"
                izz="0.1"
            />
            <origin xyz='0 0 0' rpy='0 0 0'/>
        </inertial>
    
        <visual name='chassis_visual'>
            <origin xyz='0 0 .05' rpy='0 0 0'/>
            <geometry>
                <box size='.5 0.2 0.05'/>
            </geometry>
        </visual>

        <collision name='chassis_collision'>
            <origin xyz='0 0 .05' rpy='0 0 0'/>
            <geometry>
                <box size='.5 0.2 0.05'/>
            </geometry>
        </collision>

        <visual name='fw_shaft_visual'>
            <origin xyz=".22 .11 -0.01" rpy="0 -.8 0"/>
            <geometry>
                <cylinder radius='0.01' length='0.2'/>
            </geometry>
        </visual>

        <collision name='fw_shaft_collision'>
            <origin xyz=".22 .11 -0.01" rpy="0 -.8 0"/>
            <geometry>
                <cylinder radius='0.01' length='0.2'/>
            </geometry>
        </collision>
        
        <visual name='fw_shaft_alt_visual'>
            <origin xyz=".22 -.11 -0.01" rpy="0 -.8 0"/>
            <geometry>
                <cylinder radius='0.01' length='0.2'/>
            </geometry>
        </visual>

        <collision name='fw_shaft_alt_collision'>
            <origin xyz=".22 -.11 -0.01" rpy="0 -.8 0"/>
            <geometry>
                <cylinder radius='0.01' length='0.2'/>
            </geometry>
        </collision>
        
        <visual name='visual'>
            <origin xyz='.3 .11 -.082' rpy='0 1.5707 1.5707'/>
            <geometry>
                <cylinder radius='0.015' length='0.025'/>
            </geometry>
        </visual>

        <collision name='collision'>
            <origin xyz='.3 .11 -.082' rpy='0 1.5707 1.5707'/>
            <geometry>
                <cylinder radius='0.015' length='0.025'/>
            </geometry>
        </collision>
        
        <visual name='visual'>
            <origin xyz='.3 -.11 -.082' rpy='0 1.5707 1.5707'/>
            <geometry>
                <cylinder radius='0.015' length='0.025'/>
            </geometry>
        </visual>

        <collision name='collision'>
            <origin xyz='.3 -.11 -.082' rpy='0 1.5707 1.5707'/>
            <geometry>
                <cylinder radius='0.015' length='0.025'/>
            </geometry>
        </collision>

        <collision name='back_caster_collision'>
          <origin xyz="-0.18 0 -0.05" rpy=" 0 0 0"/>
          <geometry>
            <sphere radius="0.0499"/>
          </geometry>
        </collision>
    
        <visual name='back_caster_visual'>
          <origin xyz="-0.18 0 -0.05" rpy=" 0 0 0"/>
          <geometry>
            <sphere radius="0.05"/>
          </geometry>
        </visual>
    
        <collision name='front_caster_collision'>
          <origin xyz="0.18 0 -0.05" rpy=" 0 0 0"/>
          <geometry>
            <sphere radius="0.0499"/>
          </geometry>
        </collision>
    
        <visual name='front_caster_visual'>
          <origin xyz="0.18 0 -0.05" rpy=" 0 0 0"/>
          <geometry>
            <sphere radius="0.05"/>
          </geometry>
        </visual>
    
        <collision name='tray'>
          <origin xyz="0.35 0 -0.085" rpy=" 0 0 0"/>
          <geometry>
            <box size='.12 0.2 0.005'/>
          </geometry>
        </collision>
    
        <visual name='tray'>
          <origin xyz="0.35 0 -0.085" rpy=" 0 0 0"/>
          <geometry>
            <box size='.12 0.2 0.005'/>
          </geometry>
        </visual>
    </link>

    <link name='camera'>
        <inertial>
            <mass value=".1"/>
            <inertia
                ixx="0.166667" ixy="0" ixz="0"
                iyy="0.166667" iyz="0"
                izz="0.166667"
            />
            <origin xyz='0 0 0' rpy='0 0 0'/>
        </inertial>

        <visual name='camera_visual'>
            <origin xyz='0 0 0' rpy='0 0 0'/>
            <geometry>
                <box size='0.025 0.05 0.05'/>
            </geometry>
        </visual>
        <collision name='camera_collision'>
            <origin xyz='0 0 0' rpy='0 0 0'/>
            <geometry>
                <box size='0.025 0.05 0.05'/>
            </geometry>
        </collision>
    </link>

    <joint name="camera_optical_joint" type="fixed">
        <origin xyz="0 0 0" rpy="-1.5707 0 -1.5707"/>
        <parent link="camera"/>
        <child link="camera_link_optical"/>
      </joint>
    
    <link name="camera_link_optical">
    </link>

	<link name='hokuyo'>
        <inertial>
            <mass value="1e-5"/>
            <origin xyz="0 0 0" rpy=" 0.1 0 0"/>
            <inertia
                ixx="1e-6" ixy="0" ixz="0"
                iyy="1e-6" iyz="0"
                izz="1e-6"
            />
        </inertial>

        <collision name='hokuyo_collision'>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <geometry>
            <box size=".1 .1 .1"/>
            </geometry>
        </collision>

        <visual name='hokuyo_visual'>
            <origin xyz="0 0 0" rpy=" 0 0 0"/>
            <geometry>
            <mesh filename="package://my_robot/meshes/hokuyo.dae"/>
            </geometry>
        </visual>
    </link>

    <joint type="fixed" name="hokuyo_joint">
        <origin xyz="0.20 0.05 0.05" rpy="0 0 0"/>
        <child link="hokuyo"/>
        <parent link="chassis"/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
    </joint>

    <link name='left_front_wheel'>
        <inertial>
            <mass value="5.0"/>
            <inertia
                ixx="0.1" ixy="0" ixz="0"
                iyy="0.1" iyz="0"
                izz="0.1"
            />
            <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
        </inertial>
        
        <visual name='visual'>
            <origin frame='0 0 0' rpy='0 1.5707 1.5707'/>
            <geometry>
                <cylinder radius='0.1' length='0.045' />
            </geometry>
            <material name="">
              <color rgba="0.75294 0.75294 0.75294 1"/>
            </material>
        </visual>

        <collision name='collision'>
            <origin frame='0 0 0' rpy='0 1.5707 1.5707'/>
            <geometry>
                <cylinder radius='0.1' length='0.045' />
            </geometry>
        </collision>
    </link>

    <link name='right_front_wheel'>
        <inertial>
            <mass value="5.0"/>
            <inertia
                ixx="0.1" ixy="0" ixz="0"
                iyy="0.1" iyz="0"
                izz="0.1"
            />
            <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
        </inertial>
        
        <visual name='visual'>
            <origin frame='0 0 0' rpy='0 1.5707 1.5707'/>
            <geometry>
                <cylinder radius='0.1' length='0.045' />
            </geometry>
            <material name="">
              <color rgba="0.75294 0.75294 0.75294 1"/>
            </material>
        </visual>

        <collision name='collision'>
            <origin frame='0 0 0' rpy='0 1.5707 1.5707'/>
            <geometry>
                <cylinder radius='0.1' length='0.045' />
            </geometry>
        </collision>
    </link>

    <joint type='continuous' name='left_front_wheel_hinge'>
        <origin xyz='.0 .125 0' ryp='0 0 0' />
        <child link='left_front_wheel'/>
        <parent link='chassis'/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <limit effort="10000" velocity="1000"/>
        <dynamics damping="1.0" friction="1.0"/>
    </joint>

    <joint type='continuous' name='right_front_wheel_hinge'>
        <origin xyz='.0 -.125 0' ryp='0 0 0' />
        <child link='right_front_wheel'/>
        <parent link='chassis'/>
        <axis xyz="0 1 0" rpy="0 0 0"/>
        <limit effort="10000" velocity="1000"/>
        <dynamics damping="1.0" friction="1.0"/>
    </joint>

    <joint name='chassis_JOINT_0_0' type='fixed'>
      <parent link='chassis'/>
      <child link='camera'/>
      <origin xyz="0.25 0 0" rpy="0 0 0"/>
      <axis xyz="0 1 0" rpy="0 0 0"/>
    </joint>

    <gazebo reference="chassis">
        <material>Gazebo/Green</material>
    </gazebo>

    <gazebo reference="camera">
        <material>Gazebo/Red</material>
    </gazebo>

</robot>